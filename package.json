{
    "name": "gulp-livereload-for-was",
    "description": "Live reload for WAS as gulp plugin.",
    "version": "1.0.2",
    "main": "gulp-livereload-for-was.js",
    "scripts": {
        "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
        "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/iamdenny/gulp-livereload-for-was.git"
    },
    "license": "MIT",
    "keywords": [
        "gulpplugin",
        "livereload",
        "was",
        "jsp",
        "tomcat"
    ],
    "dependencies": {
        "gulp-util": "~2.2.13",
        "lodash.merge": "^2.4.1",
        "tiny-lr": "^0.0.7",
        "through2": "~0.6.1"
    },
    "devDependencies": {
        "should": "^3.3.0",
        "mocha": "*",
        "sinon": "^1.10.2",
        "pem": "^1.4.0",
        "coveralls": "*",
        "mocha-lcov-reporter": "*",
        "istanbul": "*",
        "event-stream": "*"
    }
}


